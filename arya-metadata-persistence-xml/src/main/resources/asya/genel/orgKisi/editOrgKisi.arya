var setKisi = function (d) {	Agem.setValue(d);	Agem.setValue({kisiId: d.kisi.kisiId});	Agem.setValue({"kisiId": d.kisi.kisiId});};var prepareKisiUrl = function () {	return "genel/orgKisi/autocomplete.do";};var prepareTCKimlikNoParameterName = function () {	return "kisiParam.tckNoParam";};$(document).ready( function () {	if ($("#orgKisiId").val()) {		var json = <bean:write name="genelOrgKisiForm" filter='false'/>		AgemWM.addQueryString(Agem.json2Param( {orgKisiId : json.orgKisiId, orgBirimIdParam:json.orgKisiId, kisiId:json.kisiId}));		var resimDegistirme = "";		<agem:access URL="genel/orgKisi/edit">		resimDegistirme = "style='cursor:pointer' onclick='Other.resimYukle(this,\"genel/kisi/edit.do?json=1&type=resim&kisiId="+json.kisi.kisiId+"\",\"99\");'";		</agem:access>		AgemWM.addSideInfo(			{				id:"kisiInfo",				info: [					{value: json.kisi.ad+" "+json.kisi.soyad, clazz: "pointer",						onclick: function () {							AgemWM.link('#genelOrgKisiSelect', {url: 'genel/orgKisi/select.do'});						}					},					{label: "T.C. Kimlik No", value: json.kisi.tckNo, clazz: "not-bold"},					{value: "<div style='width:90px; height:120px;margin-top: 10px; margin-bottom: 10px; overflow:hidden'><img resimId='"+(json.kisi.resimId ?json.kisi.resimId : '')+ "' src='getDosya.do?dosyaId="+json.kisi.resimId+"' width='90' "+resimDegistirme+"/></div>"}				],				type: 1			}		);	}})function ozgecmis() {	if (Agem.byId('orgKisiId') && Agem.byId('orgKisiId').value) {		Other.metinGir(710, 600, 'Kısa Özgeçmiş', {orgKisiId: Agem.byId('orgKisiId').value, tur: "kisiOzgecmis", height: 410, width: 710});	}}	</script>
