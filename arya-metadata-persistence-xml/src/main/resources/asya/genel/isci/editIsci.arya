	<script>
$(document).ready( function () {	Agem.addPreviousNextButtons();	AgemTable.loadOnRowSelect('uyelikTable');		AgemTable.loadOnRowSelect('calisanTable');	Agem.byId("kisi.tckNo").onblur = tckNoValidation;	if ($("#kisiId").val()) {		var json = <bean:write name="genelIsciForm" filter='false'/>		AgemWM.addQueryString(Agem.json2Param( {kisiId: json.kisi.kisiId, kisiIdParam: json.kisi.kisiId}));		AgemWM.addSideInfo(			{				id:"isciInfo",				info: [					{value: json.kisi.ad+" "+json.kisi.soyad, clazz:"pointer", 						onclick: function () {							AgemWM.link('#genelIsciSelect', {url: 'genel/isci/select.do'});						}					},					{label: "T.C.K. No", value: json.kisi.tckNo, clazz: "not-bold"}				],				type: 1			}		);		tckNoValidation();	}	else{		AgemWM.removeSideInfo({type: 1, id: "isciInfo"});	}		if ($('#kisi\\.tckNo').is('[readonly="true"]')) {		$('#silBtn').hide();	}});var tckNoValidation = function (){	var tckNo = Agem.byId("kisi.tckNo").value;	if (!AgemValidation.tckNoValidate(tckNo)) {		Agem.byId("tckNoBilgiDiv").innerHTML = "<span class='agem-error bold'>Geçersiz T.C. Kimlik No</span>";	}	else{		Agem.byId("tckNoBilgiDiv").innerHTML = "<span class='agem-error bold'></span>";	}	}var dosyaEkle = function() {	if (Agem.byId('kisiId') && Agem.byId('kisiId').value) {		AgemWM.window(				{					modal : true,					label: " İşçi Bilgileri - Ekli Dosyalar", 					url:"genel/nesneDosya/list.do?pageSize=-1&nesneKodParam=kisiId&nesneIdParam="+Agem.byId('kisiId').value,					id : "nesneDosyaWindow",					width: 800, 					height: 600								});	}		};var yeniUye = function(){	AgemWM.window({		link:"genel/uye/select.do?_YENI_KAYIT=1&kisiId="+$("#kisiId").val(), 		label: "Üye İşlemleri",		width: 800, 		height: 600});}var CURRENT_TABLE = null;var setCurrentTable = function(x){CURRENT_TABLE=x;}AgemTable.CALISAN_DATA = {};AgemTable.UYE_DATA = {};AgemTable.rowDblClick = function (id){	if(CURRENT_TABLE == 'U')		AgemWM.window({link:"genel/uye/select.do?uyeId="+id, label: "Üyelik İşlemleri",width: 800, height: 600});	else if(CURRENT_TABLE == 'C'){			AgemWM.link('#genelIsciCalisanList', {url: 'genel/isciCalisan/list.do?kisiId='+AgemTable.CALISAN_DATA[id].kisiId+"&calisanId="+AgemTable.CALISAN_DATA[id].calisanId})	}}function isciSil() {    var f = document.forms['genelIsciForm'];	var d = f.action.split('/');	d.pop();	f.action = d.join('/')+"/delete.do";	if(confirm("Bu kayd\u0131 silmek istedi\u011finizden emin misiniz? \r\nBu kaydı sildiğinizde işçiye ait tüm çalışma hayatı, üyelik bilgileri ve aidat kayıtları da silinecektir!..")){	            Agem.message(AgemForm.SUBMIT_MESSAGE);        if (f.target != "_self" && !f.target)	        AgemForm.disableButtons();        AgemForm.serverSideValidation(f, function () {        	AgemForm._submit(f);        });    }    return true;}<agem:access URL="genel/csgbIsyeriUye/list">function csgb() {	if ($('#kisi\\.tckNo').val()) {		AgemWM.window( { url:'genel/csgbIsyeriUye/list.do?listeTurParam=0&tckNoParam='+$('#kisi\\.tckNo').val(), refresh:true, width: 800, height: 600, label: ""});	}}</agem:access>AgemTable.UYE_DATA[<bean:write name="uyelikTable" property="id"/>]=<%=AgemUtils.jsObject(uyelikTable)%>;AgemTable.CALISAN_DATA[<bean:write name="calisanTable" property="id"/>]=<%=AgemUtils.jsObject(calisanTable)%>;	</script>
