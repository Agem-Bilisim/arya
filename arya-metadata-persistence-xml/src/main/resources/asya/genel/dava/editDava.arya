	<script>
$(document).ready( function () {	if ($("#davaId").val()) {		// Agem.addMaskEvents("isyeri.sgkNo");// 		Agem.addMaskEvents("davaSurec.gelisme");		AgemWM.addQueryString(Agem.json2Param( {davaIdParam: $("#davaId").val(), davaId: $("#davaId").val()}));		var json = <bean:write name="genelDavaForm" filter='false'/> 		AgemWM.addSideInfo(			{				id:"davaInfo",				info: [					{value: json.mahkemesi + " - " + json.esasNo, clazz: "pointer", 							onclick: function () {								AgemWM.link('#genelDavaSelect', {url: 'genel/dava/select.do'});							}						},					{label: "Dava Tarihi", value: json.davaTarihi, clazz:"not-bold"},					{label: "", value: json.davaAdi, clazz:"not-bold"}				],				type: 1			}		);		if(<%=request.getParameter("davaSurecId")%>)			AgemWM.link('#genelDavaSurecList', {url: 'genel/davaSurec/list.do?davaSurecId='+<%=request.getParameter("davaSurecId")%>});	}	else{		AgemWM.removeSideInfo({type: 1, id: "davaInfo"});	}});var prepareDavaSurecUrl = function() {	return  'genel/davaSurec/autocomplete.do'; };var setDavaSurec = function (d) {	Agem.setValue(			{davaId: d.davaId, "davaSurec.gelisme" : d.gelisme  });};function dosyaEkle() {	if (Agem.byId('davaId') && Agem.byId('davaId').value) {		AgemWM.window(				{					modal : true,					label: "Dava Bilgileri - Ekli Dosyalar", 					url:"genel/nesneDosya/list.do?pageSize=-1&nesneKodParam=davaId&nesneIdParam="+Agem.byId('davaId').value,					id : "nesneDosyaWindow",					width: 800, 					height: 600								});	}		};	</script>
