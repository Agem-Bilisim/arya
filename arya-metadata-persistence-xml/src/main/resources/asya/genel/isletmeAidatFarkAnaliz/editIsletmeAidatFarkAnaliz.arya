	<script>
Other.KULLANICI_BILGI_TEK_SATIR = true;$(document).ready(		function () {						var unvan = '<bean:write name="isyeriUnvan"/>';			var sgk = '<bean:write name="isyeriSgkNo"/>';			if ($("#isyeriId").val()) {				unvan = (unvan.length < 35)? unvan : unvan.substring(0,unvan.lastIndexOf(' ',33));				AgemWM.addSideInfo(				{					id:"isletmeAidatFarkAnalizInfo",					info: [						{							value: unvan, clazz: "pointer",							onclick: function () {								AgemWM.link('#genelIsletmeAidatFarkAnalizSelect', {url: 'genel/isletmeAidatFarkAnaliz/select.do'});							}						},							{label: "SGK No", value: sgk, clazz:"not-bold"},							{label: "Dönem", value: ($("#donemYil").val() +'-' + $("#donemAy").val()), clazz:"not-bold"},						],						type: 1					}				);					}					});function sendikaGuncelle() {	if (!confirm('Sendika bilgilerini tekrar oluşturmak istediğinizden emin misiniz ? (Varolan bilgiler silinecektir)')) {		return;	}	Agem.perde(true, null, 'Lütfen bekleyin...');	$.ajax({"url":"genel/isletmeAidat/select.do",		data:{ merkezId: $("#isyeriId").val(), donemAy: $("#donemAy").val(), donemYil: $("#donemYil").val(), sendikaAidatGuncelle:"true",json:1 },		async : false,		timeout: 30000, 		dataType:"json", 		success:function(data){		console.log(data);			if (!data['@hata']) {								$('#sendikaUyeSayi').val(data.sendikaUyeSayi);				$('#sendikaUyeTutar').val(data.sendikaUyeTutar);				$('#sendikaDayanismaSayi').val(data.sendikaDayanismaSayi);				$('#sendikaDayanismaTutar').val(data.sendikaDayanismaTutar);				$('#sendikaSayi').val(data.sendikaSayi);				$('#sendikaTutar').val(data.sendikaTutar);			} else {				Agem.error(data['@message'],'x');			}			Agem.perde(false);		},		error:function(xhr,err,e){ 	Agem.perde(false); alert( "Hata: " + err );}});};function sayfaYenile() {	window.location = 'genel/isletmeAidatFarkAnaliz/select.do?merkezIdParam=' + $("#isyeriId").val() + '&donemAyParam=' + $("#donemAy").val() + '&donemYilParam=' + $("#donemYil").val();}<agem:access URL="genel/aidat/select">var isletmeAidatIslemleri = function () {	AgemWM.window({ link:"genel/aidat/select.do?merkezIdParam=" + $("#isyeriId").val() + "&donemAyParam=" + $("#donemAy").val() + "&donemYilParam=" + $("#donemYil").val() + "&merkezAidatParam=1", label: "Aidat İşlemleri",width: 800, height: 600});	};</agem:access>var zamAnalizRaporu = function() {<agem:access URL="rapor/zamFark/report">	var donemAy = $('#donemAy').val();	var donemYil = $('#donemYil').val();	var isyeriId = $('#isyeriId').val();	var zamDonemi = $('#zamDonemi').val();	var url = "rapor/zamFark/report.do?donemYil="+donemYil+"&donemAy="+donemAy+"&isyeriId="+isyeriId+"&zamDonemi="+zamDonemi;	Other.submitRaporForm(null,url);</agem:access><agem:access not="true" URL="rapor/zamFark/report">alert('Bu raporu çalıştırmaya yetkiniz yoktur... Lütfen sistem yöneticisine danışınız...');</agem:access>};	</script>
