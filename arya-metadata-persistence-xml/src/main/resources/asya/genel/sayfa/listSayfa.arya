	<script>
var sayfaId = ('<bean:write name="p_cleanId"/>' == '1') ? '' : '<bean:write name="p_sayfaId"/>';var subeParam = '<bean:write name="p_subeParam"/>';var header = '<agem:dictValue collection="<%=Constant.APP_SAYFA_TURLERI%>" parameter="sayfaTurParam"/>';<%	String p_name = request.getParameter("name");%>var y = 0;$(document).ready(	function () {		$('button[id$=InputBtn]').attr("disabled", true);		Agem.addElement('input','forward', {type:'hidden', name:'forward', value:'list', noClear:"1"}, null, document.forms['genelSayfaForm']);		Agem.addElement('input','pageSize', {type:'hidden', name:'pageSize', value:'-1', noClear:"1"}, null, document.forms['genelSayfaForm']);		Agem.addElement('input','name', {type:'hidden', name:'name', value:'<%=p_name%>', noClear:"1"}, null, document.forms['genelSayfaForm']);		Agem.addElement('input','sayfaTurParam', {type:'hidden', id:'sayfaTurParam', name:'sayfaTurParam', value:'<bean:write name="p_sayfaTurParam"/>', noClear:"1" }, null, document.forms['genelSayfaForm']);		Agem.byId("sayfaTur").value = '<bean:write name="p_sayfaTurParam"/>';		<%if (request.getAttribute(AgemConstant.AGEM_MESSAGE) != null) {%>			Agem.message('<%=request.getAttribute(AgemConstant.AGEM_MESSAGE)%>');		<%}%>		$(".display_scroll_div").height($(window).height() - $("form").height() -20);		var h =0;		$("#tbody tr").each (			function() {				h += $(this).height();			}		);		if (($(window).height() - $("form").height() - 20 - 24) < h) {			$("#tbody").height($(window).height() - $("form").height() - 20 - 24);		}		$("#genelSayfaForm h1 span").html(header + " - Bilgi Girişi/Güncelleme");		if (sayfaId == "") {			if (AgemTable.DATA[0]) {				AgemTable.rowClick("0");			} else {				yeniKayit();				girisDuzenleBySayfaTur();			}		} else {			kayitGetir(sayfaId,$('#orgBirimId').val());		}		<agem:access not="true" URL='<%="web/"+p_name+"/edit"%>'>		$("#kaydetBtn").remove();		y = 1;		</agem:access>		<agem:access not="true" URL='<%="web/"+p_name+"/delete"%>'>		$("#silBtn").remove();		</agem:access>		<agem:access not="true" URL='<%="web/"+p_name+"/empty"%>'>		$("#yeniBtn").remove();		</agem:access>		<agem:access not="true" right='<%="web."+p_name+".super"%>'>			$("#agem_label_input_div_sayfaTur").hide();			$("#agem_label_input_div_yayinla").hide();			$("#agem_label_input_div_anaSayfaYayinla").hide();			$("#agem_label_input_div_yayinSube").hide();		</agem:access>	});$(window).resize( function()  {	var h = 0;	$(".display_scroll_div").height($(window).height() - $("form").height() - 20);	$("#tbody tr").each (		function() {			h += $(this).height();		}	);	if (($(window).height() - $("form").height() - 20 - 24) < h) {		$("#tbody").height($(window).height() - $("form").height() - 20 - 24);	}	else {		$("#tbody").css("height", "");	}});function kayitGetir(sayfaId, orgBirimId) {	Agem.ajax("genel/sayfa/select.do", {json:1,sayfaId:sayfaId,orgBirimId:orgBirimId}, function (x) {		yeniKayit();		Agem.setValue(x);		console.log(x);		girisDuzenleBySayfaTur();		Other.writeKullaniciBilgi();		<agem:access not="true" right='<%="web."+p_name+".super"%>'>		if (x.yayinla == 1) {			$(":input").attr("disabled", "disabled");			y = "1";		}		else {			$(":input").removeAttr("disabled");			y = "0";		}		$("#tamamBtn").removeAttr("disabled");		$("#yeniBtn").removeAttr("disabled");		</agem:access>		<agem:access right='<%="web."+p_name+".super"%>'>		$(":input").removeAttr("disabled");		y = "0";		</agem:access>		$('button[id$=InputBtn]').removeAttr("disabled");	});}AgemTable.rowClick = function(id) {	if (!id) {		id = AgemTable.SELECTED_ID;		if (id == null) {			Agem.error("Lütfen kayıt seçiniz.");			return;		}	}	kayitGetir(AgemTable.DATA[id].id, AgemTable.DATA[id].orgBirimId);}function yeniKayit() {	AgemForm.temizle();	$('#yayinla').attr("checked","");	$('#guncel').val("0");	$(":input").removeAttr("disabled");	y = "0";	$('button[id$=InputBtn]').attr("disabled", true);	<% if (AsyaUtil.getInstance().merkezSube()) { %> $('#subeParam').val(subeParam); <% } %>}function kayitSil(id, f) {	idVal = $("#" + id).val();	if (!idVal) {		Agem.error("Lütfen kayıt seçiniz.");		return;	}	var x = [			'forward=list',			'sayfaId='+idVal,			<% if (AsyaUtil.getInstance().merkezSube()) { %> 'subeParam=' + subeParam, <% } %>			'pageSize=-1',			'cleanId=1',			'sayfaTurParam=' + $("#sayfaTurParam").val(),			'name=<%=p_name%>'		];	if (confirm('Kaydı silmek istediğinizden emin misiniz?'))		window.location = 'genel/sayfa/delete.do?'+x.join('&');}function sayfaResimleri() {	if (Agem.byId('sayfaId') && Agem.byId('sayfaId').value) {		AgemWM.window(				{					modal : true,					label: header + " - Foto Galeri",					url:"genel/sayfaResim/list.do?pageSize=-1&sayfaIdParam="+Agem.byId('sayfaId').value+ "&_y="+y,					id : "sayfaResimWindow",					width: 800,					height: 600				});	}}function sayfaVideolari() {	if (Agem.byId('sayfaId') && Agem.byId('sayfaId').value) {		AgemWM.window(				{					modal : true,					label: header +" - Video Galeri",					url:"genel/sayfaVideo/list.do?pageSize=-1&sayfaIdParam="+Agem.byId('sayfaId').value+ "&_y="+y,					id : "sayfaVideoWindow",					width: 800,					height: 600				});	}}function sayfaDosyalari() {	if (Agem.byId('sayfaId') && Agem.byId('sayfaId').value) {		AgemWM.window(				{					modal : true,					label: header + " - Ekli Dosyalar",					url:"genel/sayfaDosya/list.do?pageSize=-1&sayfaIdParam="+Agem.byId('sayfaId').value+ "&_y="+y,					id : "sayfaDosyaWindow",					width: 800,					height: 600				});	}}function sayfaTurChanged() {}function yayinSubeChange() {	if ($('#yayinSube').val() == '<%=AsyaUtil.getInstance().getMerkezSubeId()%>') {		$('#agem_label_input_div_anaSayfaYayinla').hide();	} else {		$('#agem_label_input_div_anaSayfaYayinla').show();	}}function girisDuzenleBySayfaTur() {	yayinSubeChange();	if ($('#sayfaTur').val() == "1" || $('#sayfaTur').val() == "2") {		$('#agem_label_input_div_anaSayfaYayinla').show();		$('#agem_label_input_div_yayinSube').show();		<% if (AsyaUtil.getInstance().merkezSube()) { %>			$('#subeFiltreBtn').show();		<% } else { %>			$('#subeFiltreBtn').hide();		<% } %>	} else {		$('#agem_label_input_div_anaSayfaYayinla').hide();//		$('#agem_label_input_div_yayinSube').hide();//		$('#subeFiltreBtn').hide();	}	<agem:access not="true" right='<%="web."+p_name+".super"%>'>	$("#agem_label_input_div_sayfaTur").hide();	$("#agem_label_input_div_yayinla").hide();	$("#agem_label_input_div_anaSayfaYayinla").hide();	$("#agem_label_input_div_yayinSube").hide();	$('#subeFiltreBtn').hide();	</agem:access>}var prepareVideoGaleriUrl = function() {	return  'genel/sayfa/autocomplete.do?turParam=9&turParam='+$("#sayfaTurParam").val();};var setVideoGaleri = function(d) {	Agem.setValue({ videoGaleriId : d.sayfaId, videoGaleriBaslik:d.baslik});}var prepareFotoGaleriUrl = function() {	return  'genel/sayfa/autocomplete.do?turParam=8&turParam='+$("#sayfaTurParam").val();};var setFotoGaleri = function(d) {	Agem.setValue({ fotoGaleriId : d.sayfaId, fotoGaleriBaslik:d.baslik});}var metinGirisi = function() {	if (Agem.byId('sayfaId') && Agem.byId('sayfaId').value) {		Other.metinGir(459, 600, header, {sayfaId: Agem.byId('sayfaId').value, tur: "sayfa", height: 370, width: 459, "_y":y});	}}var onIzleme = function() {	Agem.ajax("encryptTimestamp.do", null, function (d) {		window.open(d.url+"?x="+d.data+"&tur="+$("#sayfaTur").val()+"&id="+$("#sayfaId").val());	});};var iletisimBilgiGir = function () {	AgemForm.open('#sayfaIletisimForm', null, "iletisim.adres1", true);};var setIlIlce = function (d) {	Agem.setValue( {		"iletisim.ilceKodu": d.ilceKodu,		"iletisim.ilKodu": d.ilKodu	});};var anketBilgiGir = function () {	if (Agem.byId('sayfaId') && Agem.byId('sayfaId').value) {		var urlString;		AgemWM.window(				{					modal : true,					resizable : false,					label: header + " - Anket Bilgisi",					url: "genel/sayfaAnket/select.do?pageSize=-1&sayfaAnketId=" + Agem.byId('sayfaAnketId').value + "&sayfaId=" + Agem.byId('sayfaId').value + "&_y="+y,					id : "sayfaAnketWindow",					width: 580,					height: 630				});	}};<% if (AsyaUtil.getInstance().merkezSube()) { %>function subeFiltrele(filtrele) {	if (!filtrele) {		AgemForm.open('genelSayfaParameterForm', null, 'subeParam', true);	} else {		window.location = 'genel/sayfa/list.do?pageSize=-1&sayfaTurParam=<bean:write name="p_sayfaTurParam"/>&name=<%=p_name%>&subeParam='+$('#genelSayfaParameterForm #subeParam').val();	}}<% } %>AgemTable.DATA[<bean:write name="n"/>]=<%=AgemUtils.jsObject(item)%>	</script>
