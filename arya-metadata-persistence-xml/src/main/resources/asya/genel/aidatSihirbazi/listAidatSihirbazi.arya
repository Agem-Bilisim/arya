<?page title="DOCSYS" ?>
<arya xmlns="aryaportal.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="aryaportal.org arya.xsd">
	<script>
$(document).ready( function () {	$(".pagelinks").hide();	<%if (!AgemUtils.isEmpty(AgemMessageUtils.getError())) {%>		Agem.message('<%=AgemMessageUtils.getError()%>');		AgemWM.close("sihirbaz");	<%} else { %>	if ($('.exportlinks').length > 0) {		$('<a/>', {     			id: 'kaydet-link',     			href: "javascript:isle()",     			title: 'Aidat Listesini İşle'     		}).html("<span title='Aidat Listesini İşle' class='export-icon save-off' onmouseover=\"Other.changeClass(this, 'save-off', 'save-on')\" onmouseout=\"Other.changeClass(this, 'save-on', 'save-off')\"></span>").prependTo('.exportlinks');				$('.save-off').parent().attr("href", "javascript:isle()");	}	<%}%>});function isle() {	var href = $("#refresh-link").attr("href");	var json = {csv: 1, json: 1};	Agem.perde(true, null, 'Tutarsızlıklar düzeltiliyor. Lütfen bekleyin...');	$.ajax({"url":href,		data: json,		timeout: 300000, 		dataType:"text", 		success:function(data){			data = eval("("+data+")");			if (!data['@hata']) {				alert(data["@message"]);				AgemWM.close();			} else {				Agem.error(data['@message'],'x');			}			Agem.perde(false);		},		error:function(xhr,err,e){ if (e) alert(e); else alert( "Kayıtlar düzenlenirken hata oluştu..."); Agem.perde(false); }	});}	</script>
</arya>
