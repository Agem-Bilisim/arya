ow();			break;		case '4':			$('#gecerlilikParam').removeAttr('disabled');			$('#basTarihParam').removeAttr('disabled');			$('#bitTarihParam').removeAttr('disabled');			$('#onayBasTarihParam').removeAttr('disabled');			$('#onayBitTarihParam').removeAttr('disabled');			$('#arsivReferansNoBasParam').removeAttr('disabled');			$('#arsivReferansNoBitParam').removeAttr('disabled');			$('#agem_label_input_div_gecerlilikParam').css('display', function(){return '';});			$('#agem_label_input_div_basTarihParam').css('display', function(){return '';});			$('#agem_label_input_div_bitTarihParam').css('display', function(){return '';});			$('#basTarihParamLabel').html("Çekilme Başvuru Baş. Tarihi");			$('#bitTarihParamLabel').html("Çekilme Başvuru Bit. Tarihi");			$("#orgutDurumParam").parent().show();			$("#cikisAciklamaParam").parent().show();				$('#agem_label_input_div_orgutDurumParam').css("top", "75px");			$('#agem_label_input_div_acikUyelikGostermeParam').css("top", "100px");			$('#agem_label_input_div_cikisAciklamaParam').css("top", "125px");			$("#acikUyelikGostermeParam").parent().show();						break;		default:			break;		}	}	};function basvuruAsamaDurumChanged() {	$('#agem_label_input_div_onayBasTarihParam').css('display', function(){return 'none';});	$('#agem_label_input_div_onayBitTarihParam').css('display', function(){return 'none';});	$('#agem_label_input_div_arsivReferansNoBasParam').css('display', function(){return 'none';});	$('#agem_label_input_div_arsivReferansNoBitParam').css('display', function(){return 'none';});	if ($("#basvuruAsamaDurum").val() == "O") {		$('#agem_label_input_div_onayBasTarihParam').css('display', function(){return '';});		$('#agem_label_input_div_onayBitTarihParam').css('display', function(){return '';});		$('#onayBasTarihParam').html("Onay Baş. Tarihi");		$('#onayBitTarihParam').html("Onay Bit. Tarihi");		$('#agem_label_input_div_arsivReferansNoBasParam').css('display', function(){return '';});		$('#agem_label_input_div_arsivReferansNoBitParam').css('display', function(){return '';});		$('#arsivReferansNoBasParam').html("Arşiv Ref. Baş. No");		$('#arsivReferansNoBitParam').html("Arşiv Ref. Bit. No");	}}function listele() {	if (!$('#tarihParam').val()) {		$('#tarihParam').val(AgemDate.bugun);	}	AgemWM.submit(null, 'genelUyeParameterForm');}function temizle(){	$("#isletmeParam").parent().hide();	AgemForm.reset('genelUyeParameterForm');	durumParamChanged();}function ilKoduDegisti() {	var ilKodu = $('#ilKoduParam').val();		$('#ilceKoduParam').children().remove();	if (ilKodu) {		Agem.ajax("genel/ilce/list.do", {ilKoduParam:ilKodu, pageSize:10000, sort:'ILCE.ILCE_ADI',dir:'asc', json:1}, function (x) {			$("#ilceKoduParam").append("<option value=''></option>");			for (var i=0; i < x.results.length; i++) {				$("#ilceKoduParam").append("<option value='"+x.results[i].ilceKodu+"'>"+x.results[i].ilceAdi+"</option>");			}		});		}}function iller_ata() {	var ids = [];	var labels = [];	$(".iller_checks:checked").each(		function () {			ids.push( $(this).val() );			labels.push( $(this).attr("label") );		}	);	$("#isyeriParam\\.ilKodlariParam").val(ids.join(","));	$("#isyeriParam\\.ilAdlariParam").val(labels.join(","));	$('#iller_form').hide();	Agem.perde(false);}function iller_temizle() {	$("#isyeriParam\\.ilKodlariParam").val("");	$("#isyeriParam\\.ilAdlariParam").val("");	$(".iller_checks:checked").each(			function () {				$(this).attr("checked", "");			}		);}	</script>
