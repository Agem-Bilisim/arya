<% String exportId = request.getParameter("DISPLAYTAG_EXPORT_ID");if (exportId == null) {%>var calisanId = ('<bean:write name="p_cleanId"/>' == '1') ? '' : '<bean:write name="p_calisanId"/>';Other.KULLANICI_BILGI_TEK_SATIR = true;$(document).ready(	function () {		Agem.addElement('input','forward', {type:'hidden', name:'forward', value:'list'}, null, document.forms['genelCalisanForm']);		Agem.addElement('input','isyeriIdParamHidden', {type:'hidden', name:'isyeriIdParam', value:'<bean:write name="p_isyeriIdParam"/>'}, null, document.forms['genelCalisanForm']);		Agem.byId("isyeriId").value = '<bean:write name="p_isyeriIdParam"/>';		<logic:notEmpty name='r_message'>			Agem.message('<bean:write name='r_message' filter='false'/>');		</logic:notEmpty>		durumParamChanged();				if (calisanId == "") {			var f = AgemTable.first();			if (f) {				AgemTable.rowClick(f.id);			} else {				$('#isciyeGitBtn').hide();			}		} else {			AgemTable.rowClick(calisanId);		}				$('#isyeriIdParam').attr("noClear","1");				if ($('.exportlinks').length > 0) {			$('<a/>', {     				id: 'arama-link',     				href: "javascript:AgemForm.open('genelCalisanParameterForm', null, '---Par', true)",     				title: 'Arama'     			}).html("<span title='Arama' class='export-icon search-off' onmouseover=\"Other.changeClass(this, 'search-off', 'search-on')\" onmouseout=\"Other.changeClass(this, 'search-on', 'search-off')\"></span>").prependTo('.exportlinks');					$('.search-off').parent().attr("href", "javascript:AgemForm.open('genelCalisanParameterForm', null, '---Par', true)");		}				divAyarla();	});AgemTable.rowClick = function(id) {	if (!id) {		id = AgemTable.SELECTED_ID;		if (id == null) {			Agem.error("Lütfen kayıt seçiniz.");			return;		}	}	Agem.ajax("genel/calisan/select.do", {json:1,calisanId:id}, function (x) {		Agem.setValue(x);		Other.writeKullaniciBilgi();	});};function yeniKayit() {	Agem.setValue(		 {			 calisanId:'',			 kisiId:'',			 "isci.kisiId":'',			 "isci.kisi.kisiId":'',			 "isci.kisi.adSoyad":'',			 "isci.kisi.tckNo":'',			 girisTarihi:'',			 cikisTarihi:'',			 unvan:'',			 aidat:'',			 sendikaTemsilci:'',			 eklemeTarihi:'',			 ekleyen:'',			 sonGunlemeTarihi:'',			 sonGunleyen:''		}	);	    $('#isciyeGitBtn').hide();		 	Agem.byId("dayanismaAidati").checked = false;	Agem.byId("ucretsizIzinde").checked = false;		Agem.byId("kapsamDisi").checked = false;	Other.writeKullaniciBilgi();}function kayitSil(id, f) {	if (!Agem.byId(id).value) {		Agem.error("Lütfen kayıt seçiniz.");		return;	}	var x = [			'forward=list',			'calisanId='+Agem.byId(id).value,			'cleanId=1',			'durumParam=' + $('#durumParam').val(),			'sendikaTemsilciParam=' + ($('#sendikaTemsilciParam').val() ? $('#sendikaTemsilciParam').val() : ""),			'cikisNedeniParam=' + ($('#cikisNedeniParam').val() ? $('#cikisNedeniParam').val() : ""),			'isyeriIdParam=<bean:write name="p_isyeriIdParam"/>'		];	if (confirm('Kaydı silmek istediğinizden emin misiniz?\r\nBu kaydı sildiğinizde çalışma kaydıyla ilişkili tüm aidat kayıtları da silinecektir!..')) 		Agem.open( 'genel/calisan/delete.do?'+x.join('&')+(($('#dayanismaAidatiParam').attr("checked")) ? '&dayanismaAidatiParam=1' : ""));}var kaydet = function(){	$('#genelCalisanForm').append("<input type='hidden' name='isyeriIdParam' id='isyeriIdParam2' value='"+$('#isyeriIdParam').val()+"'/>");	$('#genelCalisanForm').append("<input type='hidden' name='durumParam' id='durumParam2' value='"+$('#durumParam').val()+"'/>");	$('#genelCalisanForm').append("<input type='hidden' name='dayanismaAidatiParam' id='dayanismaAidatiParam2' value='"+($('#dayanismaAidatiParam').val()) +"'/>");	$('#genelCalisanForm').append("<input type='hidden' name='mevsimlikDaimiParam' id='mevsimlikDaimiParam2' value='"+($('#mevsimlikDaimiParam').val()) +"'/>");	$('#genelCalisanForm').append("<input type='hidden' name='ucretsizIzindeParam' id='ucretsizIzindeParam2' value='"+($('#ucretsizIzindeParam').val()) +"'/>");	$('#genelCalisanForm').append("<input type='hidden' name='kapsamDisiParam' id='kapsamDisiParam2' value='"+($('#kapsamDisiParam').val()) +"'/>");	$('#genelCalisanForm').append("<input type='hidden' name='sendikaTemsilciParam' id='sendikaTemsilciParam2' value='"+$('#sendikaTemsilciParam').val()+"'/>");	$('#genelCalisanForm').append("<input type='hidden' name='cikisNedeniParam' id='cikisNedeniParam2' value='"+$('#cikisNedeniParam').val()+"'/>");		AgemForm.submit('calisanId', 'genelCalisanForm');}var setKisi = function (d) {	Agem.setValue(d.kisi, "isci.kisi");	$('#kisiId').val(d.kisi.kisiId);	$('#isci\\.kisiId').val(d.kisi.kisiId);	$('#isci\\.cinsiyet').val(d.cinsiyet);};var prepareKisiUrl = function () {	return "genel/isci/autocomplete.do?RxdE12=1";};var prepareAdSoyadParameterName = function () {	return "kisiParam.adSoyadParam";};var prepareTCKimlikNoParameterName = function () {	return "kisiParam.tckNoParam";};durumParamChanged = function(){	switch($('#durumParam').val()){	case '2':		$('#agem_label_input_div_cikisNedeniParam').css('display', function(){return ''});		break;	default:		$('#agem_label_input_div_cikisNedeniParam').css('display', function(){return 'none'});		$('#cikisNedeniParam').val('');		break;	}};function isciyeGit(id) {	var id = $('#' +id).val();	if (!id) {		Agem.error("Lütfen kayıt seçiniz.");		return;	}	AgemWM.window({ link:"genel/isci/select.do?kisiId="+id + '&kisiIdParam='+id, label: "İşçi Bilgileri",width: 800, height: 600});}var raporAl = function(){	Agem.open("rapor/isyeriCalisan/report.do?isyeriIdParam="+$('#isyeriId').val()+"&r="+Math.random());}<%}%>$(window).resize( function () {	divAyarla();});var divAyarla = function () {	var w = $(window).width();	$("#listeDis").width(w-25);};	</script>
