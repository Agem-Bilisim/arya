	<script>
var setCinsi = function(d) {	Agem.setValue({ cinsi : d.cinsi});};var prepareGidenEvrakUrl = function() {	return  'genel/gidenEvrak/autocomplete.do'; };$(document).ready(	function () {		if ($("#evrakId").val()) {			AgemWM.addQueryString(Agem.json2Param( {evrakIdParam: $("#evrakId").val(), evrakId: $("#evrakId").val()}));			var json = <bean:write name="genelGidenEvrakForm" filter='false'/> 			AgemWM.addSideInfo(				{					id:"gidenEvrakInfo",					info: [						{label: "Sıra No", value: json.siraNo, clazz: "pointer", 							onclick: function () {								AgemWM.link('#genelGidenEvrakSelect', {url: 'genel/gidenEvrak/select.do'});							}						},						{label: "Gidiş Tarihi", value: json.gelisTarihi, clazz:"not-bold"},						{label: "Kime", value: json.kimden, clazz:"not-bold"},						{label: "Konu", value: json.konu, clazz:"not-bold"}					],					type: 1				}			);			$('#gecikmeMsg').css('top',parseInt($('#agem_label_input_div_evrakSure').css('top'))+8);			if (json.gecikme == 'gecikme') {				$('#gecikmeMsg').html('Evrağın işlem tarihi geçmiş ya da son gününde...');			} else {				$('#gecikmeMsg').html('');			}					} else {			$('#agem_label_input_div_dosyaEkleBtn').hide();			AgemWM.removeSideInfo({type: 1, id: "gidenEvrakInfo"});		}		$('#genelGidenEvrakForm .agem-form-div').append('<div id="siraNoAciklamaDiv" style="position:absolute; color:#942730">Boş geçilirse sıradaki numara verilecektir</div>');		$('#siraNoAciklamaDiv').css('top', function() { return $('#agem_label_input_div_siraNo').position().top+7; });		$('#siraNoAciklamaDiv').css('left', function() { return $('#agem_label_input_div_siraNo').position().left+$('#agem_label_input_div_siraNo').width() + 5;});	});function dosyaEkle() {	if (Agem.byId('evrakId') && Agem.byId('evrakId').value) {		AgemWM.window(				{					modal : true,					label: " Giden Evrak - Ekli Dosyalar", 					url:"genel/nesneDosya/list.do?pageSize=-1&nesneKodParam=evrakId&nesneIdParam="+Agem.byId('evrakId').value,					id : "nesneDosyaWindow",					width: 800, 					height: 600								});	}		};function setKategori(d) {	if (d && d.value) {		$('#kategoriKod').val(d.value);				$('#kategoriAciklama').val(d.label);	}}	</script>
