	<script>
var isyeriKontakId = ('<bean:write name="p_cleanId"/>' == '1') ? '' : '<bean:write name="p_isyeriKontakId"/>';Other.KULLANICI_BILGI_TEK_SATIR = true;$(document).ready(	function () {		Agem.addElement('input','forward', {type:'hidden', name:'forward', value:'list'}, null, document.forms['genelIsyeriKontakForm']);		Agem.addElement('input','pageSize', {type:'hidden', name:'pageSize', value:'-1'}, null, document.forms['genelIsyeriKontakForm']);		Agem.addElement('input','isyeriIdParam', {type:'hidden', name:'isyeriIdParam', value:'<bean:write name="p_isyeriIdParam"/>'}, null, document.forms['genelIsyeriKontakForm']);		Agem.byId("isyeriId").value = '<bean:write name="p_isyeriIdParam"/>';		<%if (request.getAttribute(AgemConstant.AGEM_MESSAGE) != null) {%>		Agem.message('<%=request.getAttribute(AgemConstant.AGEM_MESSAGE)%>');		<%}%>				var isyeriKontakId = $('#isyeriKontakId').val();		if (!isyeriKontakId ) {			var f = AgemTable.first();			if (f) {				AgemTable.rowClick(f.id);			} 		} else {			kayitGetir(isyeriKontakId );		}	});function kayitGetir(id) {	Agem.ajax("genel/isyeriKontak/select.do", {json:1,isyeriKontakId:id}, function (x) {		yeniKayit();		Agem.setValue(x);		Other.writeKullaniciBilgi();	});}AgemTable.rowClick = function(id) {	if (!id) {		id = AgemTable.SELECTED_ID;		if (id == null) {			Agem.error("Lütfen kayıt seçiniz.");			return;		}	}		kayitGetir(AgemTable.DATA["d-"+id].id);};function yeniKayit() {	Agem.setValue(		 {			isyeriKontakId:'',			unvan:'',			adi:'',			soyad:'',			iletisimId:'',			"iletisim.iletisimId":'',			eklemeTarihi:'',			ekleyen:'',			sonGunlemeTarihi:'',			sonGunleyen:'',			kategori:'',			"iletisim.tel":'',			"iletisim.cep":'',			"iletisim.faks":'',			"iletisim.eposta":'',			"iletisim.adres1":'',			"iletisim.adres2":'',			"iletisim.postaKodu":'',			"iletisim.ilceKodu":'',			"iletisim.ilKodu":'',			"iletisim.ilce.value":'',						"iletisim.aciklama":''		}	);	Other.writeKullaniciBilgi();}function kayitSil(id, f) {	if (!Agem.byId(id).value) {		Agem.error("Lütfen kayıt seçiniz.");		return;	}	var x = [			'forward=list',			'isyeriKontakId='+Agem.byId(id).value,			'iletisimId='+Agem.byId('iletisimId').value,			'iletisim.iletisimId='+Agem.byId('iletisim.iletisimId').value,			'cleanId=1',			'isyeriIdParam=<bean:write name="p_isyeriIdParam"/>'		];	if (confirm('Kaydı silmek istediğinizden emin misiniz?')) 		Agem.open( 'genel/isyeriKontak/delete.do?'+x.join('&'));}var setIlIlce = function (d) {	Agem.setValue( {		"iletisim.ilceKodu": d.ilceKodu,		"iletisim.ilKodu": d.ilKodu	});};	</script>
