	<script>
Other.KULLANICI_BILGI_TEK_SATIR = true;$(document).ready(		function () {			if ($("#aidatId").val()) {				AgemWM.addQueryString(Agem.json2Param( {aidatIdParam: $("#aidatId").val(), aidatId: $("#aidatId").val()}));				var json = <bean:write name="genelAidatFarkAnalizForm" filter='false'/> 				var json = <bean:write name="genelAidatFarkAnalizForm" filter='false'/> 																var unvan = (json.isyeri.unvan.toString().length < 35)? json.isyeri.unvan : json.isyeri.unvan.toString().substring(0,json.isyeri.unvan.toString().lastIndexOf(' ',33));				AgemWM.addSideInfo(				{					id:"aidatFarkAnalizInfo",					info: [						{							value: unvan, clazz: "pointer",							onclick: function () {								AgemWM.link('#genelAidatFarkAnalizSelect', {url: 'genel/aidatFarkAnaliz/select.do'});							}						},							{label: "SGK No", value: json.isyeri.sgkKisaNo, clazz:"not-bold"},							{label: "Dönem", value: (json.donemYil +'-' + json.donemAy), clazz:"not-bold"},						],						type: 1					}				);					}			if ($('#zamDonemi').val()) {				$('#zamDonemiDiv').html("<span class='agem-mandatory'>Zam Dönemi &nbsp;&nbsp;&nbsp;</span> <a href='javascript:zamAnalizRaporu()'>"+$('#zamDonemi').val()+"</a>");			} else {				$('#zamDonemiDiv').html("");			}		});function sendikaGuncelle() {	if (!confirm('Sendika bilgilerini tekrar oluşturmak istediğinizden emin misiniz ? (Varolan bilgiler silinecektir)')) {		return;	}	Agem.perde(true, null, 'Lütfen bekleyin...');	$.ajax({"url":"genel/aidatFarkAnaliz/select.do",		data:{ aidatId: $("#aidatId").val(), sendikaGuncelle:"true",json:1 },		async : false,		timeout: 30000, 		dataType:"json", 		success:function(data){			if (!data['@hata']) {				$('#sendikaUyeSayi').val(data.sendikaUyeSayi);				$('#sendikaUyeTutar').val(data.sendikaUyeTutar);				$('#sendikaDayanismaSayi').val(data.sendikaDayanismaSayi);				$('#sendikaDayanismaTutar').val(data.sendikaDayanismaTutar);				$('#sendikaSayi').val(data.sendikaSayi);				$('#sendikaTutar').val(data.sendikaTutar);			} else {				Agem.error(data['@message'],'x');			}			Agem.perde(false);		},		error:function(xhr,err,e){ 	Agem.perde(false); alert( "Hata: " + err );}});};function sayfaYenile() {	window.location = 'genel/aidatFarkAnaliz/select.do?aidatId=' + $('#aidatId').val();}<agem:access URL="genel/aidat/select">var aidatIslemleri = function () {	AgemWM.window({ link:"genel/aidat/select.do?aidatId="+$("#aidatId").val() + "&aidatIdParam="+$("#aidatId").val(), label: "Aidat İşlemleri",width: 800, height: 600});	};</agem:access>var zamAnalizRaporu = function() {<agem:access URL="rapor/zamFark/report">	var donemAy = $('#donemAy').val();	var donemYil = $('#donemYil').val();	var isyeriId = $('#isyeriId').val();	var zamDonemi = $('#zamDonemi').val();	var url = "rapor/zamFark/report.do?donemYil="+donemYil+"&donemAy="+donemAy+"&isyeriId="+isyeriId+"&zamDonemi="+zamDonemi;	Other.submitRaporForm(null,url);</agem:access><agem:access not="true" URL="rapor/zamFark/report">alert('Bu raporu çalıştırmaya yetkiniz yoktur... Lütfen sistem yöneticisine danışınız...');</agem:access>};	</script>
