	<script>
AgemForm.submitMessage('Arama yapılıyor...');Agem.ready(	function () {		orgTarihiKontrol();		$("#orgutluParam").change(orgTarihiKontrol);	});	var setIlIlceParam = function (d) {	Agem.setValue( {		"iletisim.ilce.param": d.value	});};var setIsyeri = function (d) {	if (d.isyeriId) {		AgemWM.addQueryString(Agem.json2Param( {isyeriIdParam: d.id, isyeriId: d.id}));		AgemWM.link('#genelIsyeriSelect', {url: 'genel/isyeri/select.do'});	}};var orgTarihiKontrol = function (){	if ($("#orgutluParam").val() == 1) {		$("#orgutlenmeTarihiBasParam").parent().show();		$("#orgutlenmeTarihiBitParam").parent().show();	}	else {		$("#orgutlenmeTarihiBasParam").val('');		$("#orgutlenmeTarihiBitParam").val('');		$("#orgutlenmeTarihiBasParam").parent().hide();		$("#orgutlenmeTarihiBitParam").parent().hide();	}}function ilKoduDegisti() {	var ilKodu = $('#ilKoduParam').val();		$('#ilceKoduParam').children().remove();	if (ilKodu) {		Agem.ajax("genel/ilce/list.do", {ilKoduParam:ilKodu, pageSize:10000, sort:'ILCE.ILCE_ADI',dir:'asc', json:1}, function (x) {			$("#ilceKoduParam").append("<option value=''></option>");			for (var i=0; i < x.results.length; i++) {				$("#ilceKoduParam").append("<option value='"+x.results[i].ilceKodu+"'>"+x.results[i].ilceAdi+"</option>");			}		});		}}function iller_ata() {	var ids = [];	var labels = [];	$(".iller_checks:checked").each(		function () {			ids.push( $(this).val() );			labels.push( $(this).attr("label") );		}	);	$("#ilKodlariParam").val(ids.join(","));	$("#ilAdlariParam").val(labels.join(","));	$('#iller_form').hide();	Agem.perde(false);}function iller_temizle() {	$("#ilKodlariParam").val("");	$("#ilAdlariParam").val("");	$(".iller_checks:checked").each(			function () {				$(this).attr("checked", "");			}		);}$(document).ready( function () {	$("#ilAdlariParam").click(function () {		AgemForm.open('#iller_form', null, null, true);		});});	</script>
