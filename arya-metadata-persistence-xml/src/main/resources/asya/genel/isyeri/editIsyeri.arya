	<script>
$(document).ready( function () {	Agem.addPreviousNextButtons();	var isletme = '<bean:write scope="request" name="isletmeAidatAc"/>';	if(isletme == '1'){		if (window.parent)			window.parent.$("#genelIsletmeAidatFarkAnalizList").parent().show();	}	else if(isletme == '0'){		if (window.parent) {			window.parent.$("#genelIsletmeAidatFarkAnalizList").parent().hide();		}	}		$('#sube').change(function() {		if ($(this).val()) $('#orgutlu').attr('checked','true').change();	});		 	if ($('#orgutlu').attr("readonly") != 'true' ){		$('#orgutlu').change(function() {			if ($(this).attr("checked"))				$("#orgutlenmeTarihi").parent().show();			else 				$("#orgutlenmeTarihi").parent().hide();		})		.change();	};		if ($("#isyeriId").val()) {		AgemWM.addQueryString(Agem.json2Param( {isyeriIdParam: $("#isyeriId").val(), isyeriId: $("#isyeriId").val()}));		var json = <bean:write name="genelIsyeriForm" filter='false'/> 						var unvan = (json.unvan.toString().length < 35)? json.unvan : json.unvan.toString().substring(0,json.unvan.toString().lastIndexOf(' ',33));		AgemWM.addSideInfo(			{				id:"isyeriInfo",				info: [					{						value: unvan, clazz: "pointer",						onclick: function () {							AgemWM.link('#genelIsyeriSelect', {url: 'genel/isyeri/select.do'});						}					},					{label: "SGK No", value: json.sgkKisaNo, clazz: "not-bold"}				],				type: 1			}		);		if ( $('#orgutlu').attr("checked")) {			$('#agem_label_input_div_orgutlenmeTarihi').show();		} else {			$('#agem_label_input_div_orgutlenmeTarihi').hide();		}	}	else{		AgemWM.removeSideInfo({type: 1, id: "isyeriInfo"});	}	if(<%=request.getParameter("isyeriZiyaretId")%>)			AgemWM.link('#genelIsyeriZiyaretList', {url: 'genel/isyeriZiyaret/list.do?isyeriZiyaretId='+<%=request.getParameter("isyeriZiyaretId")%>});});var setIlIlce = function (d) {	Agem.setValue( {		"iletisim.ilceKodu": d.ilceKodu,		"iletisim.ilKodu": d.ilKodu	});};var dosyaEkle = function() {	if (Agem.byId('isyeriId') && Agem.byId('isyeriId').value) {		AgemWM.window(				{					modal : true,					label: " İşyeri Bilgileri - Ekli Dosyalar", 					url:"genel/nesneDosya/list.do?pageSize=-1&nesneKodParam=isyeriId&nesneIdParam="+Agem.byId('isyeriId').value,					id : "nesneDosyaWindow",					width: 800, 					height: 600								});	}		};var csgbyeGit = function() {	if (Agem.byId("isyeriId").value)		AgemWM.window( {			link: "genel/csgbIsyeri/select.do?isyeriIdSendika="+Agem.byId("isyeriId").value,             width: 800, height: 600,             label: "ÇSGB Entegrasyon"});		};var isciSayisiDuzenle = function() {	if (Agem.byId("isyeriId").value)		AgemWM.window({			modal : true,			url : "genel/isciSayisi/list.do?isyeriIdParam=" + Agem.byId("isyeriId").value,			width : 620,			height : 600,			label : "İşçi Sayısı Düzenle"		});};var sil = function(){	if($("#merkez").val()=="true"){		alert("Bu işyerine bağlı alt işyerleri bulunmaktadır. Bu işyerini silmeden önce alt işyerlerini silmeniz gerekiyor.");		return;	}	if($("#calisaniVar").val()){		if (!confirm('İşyerinde çalışan işçiler bulunmaktadır. İşyeri ve çalışan kayıtlarını silmek istediğinizden emin misiniz?')) {			return;		}	}		AgemForm.del('isyeriId', 'genelIsyeriForm');};	</script>
