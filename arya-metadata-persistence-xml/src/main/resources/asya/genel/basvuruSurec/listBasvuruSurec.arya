	<script>
Other.KULLANICI_BILGI_TEK_SATIR = true;var _SUREC_LISTE = []; var basvuruSurecId = ('<bean:write name="p_cleanId"/>' == '1') ? '' : '<bean:write name="p_basvuruSurecId"/>';$(document).ready(	function () {		Agem.addElement('input','forward', {type:'hidden', name:'forward', value:'list', noClear:"1"}, null, document.forms['genelBasvuruSurecForm']);		Agem.addElement('input','pageSize', {type:'hidden', name:'pageSize', value:'-1', noClear:"1"}, null, document.forms['genelBasvuruSurecForm']);		Agem.addElement('input','basvuruIdParam', {type:'hidden', name:'basvuruIdParam', value:'<bean:write name="p_basvuruIdParam"/>', noClear:"1"}, null, document.forms['genelBasvuruSurecForm']);		$("#basvuruId").val('<bean:write name="p_basvuruIdParam"/>');		<%if (request.getAttribute(AgemConstant.AGEM_MESSAGE) != null) {%>		Agem.message('<%=request.getAttribute(AgemConstant.AGEM_MESSAGE)%>');		<%}%>				$('#yeniSurecDurumDiv').hide();				if (basvuruSurecId == "") {			if (AgemTable.DATA[0]) {				AgemTable.rowClick("0");			} 		} else {			kayitGetir(basvuruSurecId);		}	});AgemTable.rowClick = function(id) {	if (!id) {		id = AgemTable.SELECTED_ID;		if (id == null) {			Agem.error("Lütfen kayıt seçiniz.");			return;		}	}		kayitGetir(AgemTable.DATA[id].id);};function kayitGetir(basvuruSurecId) {		Agem.ajax("genel/basvuruSurec/select.do", {json:1,basvuruSurecId:basvuruSurecId}, function (x) {		AgemForm.temizle();		Agem.setValue(x);		eval('_SUREC_LISTE = ' + x.sonrakilerJSONArray +';');		if ($('#enSonuncuMu').val() == 'Y') {			if (_SUREC_LISTE.length > 0 ) {								AgemForm.setComboContent("yeniSurecDurumId", _SUREC_LISTE);				$('#yeniSurecDurumDiv').show();			}			if (x.sonlanmaTarihi) {				$('#sonlanmaTarihiDiv').html('Başvuru durumu ' + x.sonlanmaTarihi + ' tarihine kadar sonuçlanmalıdır.');				if (x.gecikme == '1') {					$('#sonlanmaTarihiDiv').css("color", "red");				} else {					$('#sonlanmaTarihiDiv').css("color", "");				}			}			$('button[id=SilBtn]').attr("disabled", true);		} else {			$('#yeniSurecDurumDiv').hide();			$('#sonlanmaTarihiDiv').html('');			$('button[id=SilBtn]').attr("disabled", false);		}		if ($('#yetkiliMi').val() == 'N') {			$('button[id$=Btn]').attr("disabled", true);		} else {			$('button[id$=Btn]').attr("disabled", false);		}				Other.writeKullaniciBilgi();	});}function yeniDurumaGec() {		if (!$('#gecisTarih').val()) {		alert("Lütfen yeni durumun tarihini giriniz...");		return;	}		$.ajax({"url":"genel/basvuruSurec/add.do",		data:{ basvuruId: $('#basvuruId').val(), surecDurumId: $('#yeniSurecDurumId').val(), gecisTarih: $('#gecisTarih').val(), json:1 },		async : false,		timeout: 30000, 		dataType:"json", 		success:function(data){						if (!data["@hata"]) {				var x = [							'forward=list',							'pageSize=-1',							'basvuruIdParam=<bean:write name="p_basvuruIdParam"/>', 							'basvuruSurecId='+data.basvuruSurecId						];<%--				window.location = 'genel/basvuruSurec/select.do?'+x.join('&');;--%>			Agem.open('genel/basvuruSurec/select.do?'+x.join('&'))			} else {				Agem.message(data["@message"],'x');			}		},		error:function(xhr,err,e){ alert( "Hata: " + err );}});}function kayitSil(id, f) {	if (!$('#'+id).val()) {		Agem.error("Lütfen kayıt seçiniz.");		return;	}	var x = [			'forward=list',			'pageSize=-1',			'basvuruIdParam=<bean:write name="p_basvuruIdParam"/>', 			'basvuruId=<bean:write name="p_basvuruIdParam"/>', 			'basvuruSurecId='+$('#'+id).val(),			'cleanId=1'		];	if (confirm('Kaydı silmek istediğinizden emin misiniz?')) 		window.location = 'genel/basvuruSurec/delete.do?'+x.join('&');};function dosyaEkle() {	if (Agem.byId('basvuruSurecId') && Agem.byId('basvuruSurecId').value) {		AgemWM.window(				{					modal : true,					label: " Başvuru Süreç Bilgileri - Ekli Dosyalar", 					url:"genel/nesneDosya/list.do?pageSize=-1&nesneKodParam=basvuruSurecId&nesneIdParam="+Agem.byId('basvuruSurecId').value,					id : "nesneDosyaWindow",					width: 800, 					height: 600								});	}		};function bilgiGoster() {		if ($('#basvuruSurecId').val()) {		AgemWM.modal(				{					label: "Süreç Bilgi", 					width: 500,					height: 500,					content: '<div style="position:absolute; left:10px;top:30px;width:470px; height:410px;border:none;overflow:auto;">' +  $('#bilgi').val() + '</div>',					buttons: [					   {						   id : 'kapat',						   label: "Kapat",						   func: function (data) { 						   				return true;						   		} 					   }					]				});	} else {		Agem.message('Herhangi bir süreçte değilsiniz','x');	}		}AgemDate.attachEvents("gecisTarih", true);AgemTable.DATA[<bean:write name="n"/>]=<%=AgemUtils.jsObject(item,1,"#id#")%>	</script>
