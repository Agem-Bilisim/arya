	<script>
var isyeriZiyaretId = ('<bean:write name="p_cleanId"/>' == '1') ? '' : '<bean:write name="p_isyeriZiyaretId"/>';Other.KULLANICI_BILGI_TEK_SATIR = true;Agem.addLoadFunction(	function () {		Agem.addElement('input','forward', {type:'hidden', name:'forward', value:'list'}, null, document.forms['genelIsyeriZiyaretForm']);		Agem.addElement('input','pageSize', {type:'hidden', name:'pageSize', value:'-1'}, null, document.forms['genelIsyeriZiyaretForm']);		Agem.addElement('input','isyeriIdParam', {type:'hidden', name:'isyeriIdParam', value:'<bean:write name="p_isyeriIdParam"/>'}, null, document.forms['genelIsyeriZiyaretForm']);		Agem.byId("isyeriId").value = '<bean:write name="p_isyeriIdParam"/>';		<%if (request.getAttribute(AgemConstant.AGEM_MESSAGE) != null) {%>		Agem.message('<%=request.getAttribute(AgemConstant.AGEM_MESSAGE)%>');		<%}%>				var isyeriZiyaretId = $('#isyeriZiyaretId').val();		if (!isyeriZiyaretId ) {			var f = AgemTable.first();			if (f) {				AgemTable.rowClick(f.id);			} 		} else {			kayitGetir(isyeriZiyaretId );		}	});function kayitGetir(id) {	Agem.ajax("genel/isyeriZiyaret/select.do", {json:1,isyeriZiyaretId:id}, function (x) {		Agem.setValue(x);		Other.writeKullaniciBilgi();	});}AgemTable.rowClick = function(id) {	if (!id) {		id = AgemTable.SELECTED_ID;		if (id == null) {			Agem.error("Lütfen kayıt seçiniz.");			return;		}	}		kayitGetir(AgemTable.DATA["d-"+id].id);};function yeniKayit() {	Agem.setValue(		 {			isyeriZiyaretId:'',		    ziyaretTarihi:'',			gidisSaati:'',			ziyaretEdenler:'',			gorusulenKisiler:'',			gorusulenKonular:'',			ziyaretinAmaci:'',			ziyaretFaaliyeti:'',			aciklama:'',			eklemeTarihi:'',			ekleyen:'',			sonGunlemeTarihi:'',			sonGunleyen:''		}	);	Other.writeKullaniciBilgi();}function kayitSil(id, f) {	if (!Agem.byId(id).value) {		Agem.error("Lütfen kayıt seçiniz.");		return;	}	var x = [			'forward=list',			'isyeriZiyaretId='+Agem.byId(id).value,			'cleanId=1',			'isyeriIdParam=<bean:write name="p_isyeriIdParam"/>'		];	if (confirm('Kaydı silmek istediğinizden emin misiniz?')) 		Agem.open( 'genel/isyeriZiyaret/delete.do?'+x.join('&'));}	</script>
