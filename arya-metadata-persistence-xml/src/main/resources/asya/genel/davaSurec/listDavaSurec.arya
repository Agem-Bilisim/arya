	<script>
Other.KULLANICI_BILGI_TEK_SATIR = true;$(document).ready(	function () {		Agem.addElement('input','forward', {type:'hidden', name:'forward', value:'list'}, null, document.forms['genelDavaSurecForm']);		Agem.addElement('input','pageSize', {type:'hidden', name:'pageSize', value:'-1'}, null, document.forms['genelDavaSurecForm']);		Agem.addElement('input','davaIdParam', {type:'hidden', name:'davaIdParam', value:'<bean:write name="p_davaIdParam"/>'}, null, document.forms['genelDavaSurecForm']);		Agem.byId("davaId").value = '<bean:write name="p_davaIdParam"/>';		<%if (request.getAttribute(AgemConstant.AGEM_MESSAGE) != null) {%>		Agem.message('<%=request.getAttribute(AgemConstant.AGEM_MESSAGE)%>');		<%} %>		var davaSurecId = $('#davaSurecId').val();		if (!davaSurecId) {			var f = AgemTable.first();			if (f) {				AgemTable.rowClick(f.id);			} 		} else {			kayitGetir(davaSurecId);		}	});function kayitGetir(davaSurecId) {	Agem.ajax("genel/davaSurec/select.do", {json:1,davaSurecId:davaSurecId}, function (x) {		Agem.setValue(x); 		Other.writeKullaniciBilgi();	});}AgemTable.rowClick = function(id) {	if (!id) {		id = AgemTable.SELECTED_ID;		if (id == null) {			Agem.error("Lütfen kayıt seçiniz.");			return;		}	}	kayitGetir(AgemTable.DATA["d-"+id].id);};function yeniKayit() {	Agem.setValue(		{			davaSurecId:'',			tarih:'',			gelisme:'',			aciklama:'',			eklemeTarihi:'',			ekleyen:'',			sonGunlemeTarihi:'',			sonGunleyen:''		}	);	Other.writeKullaniciBilgi();}function kayitSil(id, f) {	if (!Agem.byId(id).value) {		Agem.error("Lütfen kayıt seçiniz.");		return;	}	var x = [			'forward=list',			'davaIdParam='+Agem.byId('davaId').value,			'davaSurecId='+Agem.byId(id).value		];	if (confirm('Kaydı silmek istediğinizden emin misiniz?')) 		Agem.open('genel/davaSurec/delete.do?'+x.join('&'));};function dosyaEkle() {	if (Agem.byId('davaSurecId') && Agem.byId('davaSurecId').value) {		AgemWM.window(				{					modal : true,					label: " Dava Süreç Bilgileri - Ekli Dosyalar", 					url:"genel/nesneDos