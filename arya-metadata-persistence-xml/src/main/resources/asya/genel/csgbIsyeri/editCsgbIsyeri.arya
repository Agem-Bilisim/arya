	<script>
$(document).ready( function () {		if ($("#isyeriId").val()) {		AgemWM.addQueryString(Agem.json2Param( {isyeriIdParam: $("#isyeriId").val(), isyeriId: $("#isyeriId").val()}));		var json = <bean:write name="genelCsgbIsyeriForm" filter='false'/> 						var unvan = (json.unvan.toString().length < 35)? json.unvan : json.unvan.toString().substring(0,json.unvan.toString().lastIndexOf(' ',33));		AgemWM.addSideInfo(			{				id:"isyeriInfo",				info: [					{						value: unvan, clazz: "pointer",						onclick: function () {						AgemWM.link('#genelCsgbIsyeriSelect', {url: 'genel/csgbIsyeri/select.do'});					}					},					{label: "İşyeri SGK No", value: json.sgkKisaNo, clazz: "not-bold"}				],				type: 1			}		);		$('#uyeSayisiLabel').html('<a href="javascript:showUyeler()">ÇSGB Üye Sayısı</a>');		if($("#isyeriIdSendika").val()){						$('#unvanSendikaLabel').html('<a href="javascript:setUnvan()">İşyeri Ünvanı</a>');			$('#adres1SendikaLabel').html('<a href="javascript:setAdres1()">Adres(1)</a>');			$('#adres2SendikaLabel').html('<a href="javascript:setAdres2()">Adres(2)</a>');			$('#telSendikaLabel').html('<a href="javascript:setTel()">Telefon</a>');			$('#faksSendikaLabel').html('<a href="javascript:setFaks()">Faks</a>');			$('#calisanSayisiSendikaLabel').html('<a href="javascript:setCalisanSayisi()">Çalışan Sayısı</a>');			$("#yukleBtn").parent().hide();		}			}	else{		AgemWM.removeSideInfo({type: 1, id: "isyeriInfo"});	}		$('#agem_label_input_div_uyeSayisi').css('left',$('#agem_label_input_div_faks').css('left'));});var setUnvan= function(){	$('#unvanSendika').val($('#unvan').val());};var setAdres1 = function (){		var adresParca = "";	if($('#adres').val().toString().length<255){		adresParca = $('#adres').val();	}	else{		adresParca = $('#adres').val().toString().substring(0,255)	}	$('#adres1Sendika').val(adresParca);};var setAdres2 = function (){	var adresParca = "";	if($('#adres').val().toString().length>510){		adresParca = $('#adres').val().toString().substring(255,510);	}	else{		adresParca = $('#adres').val().toString().substring(255);	}	$('#adres2Sendika').val(adresParca);};var setTel= function(){	$('#telSendika').val($('#tel').val());};var setFaks= function(){	$('#faksSendika').val($('#faks').val());};var setCalisanSayisi= function(){	$('#calisanSayisiSendika').val($('#calisanSayisi').val());};var isyerineGit = function() {	if ($("#isyeriIdSendika").val())		AgemWM.window( {link: "genel/isyeri/select.do?isyeriId="+$("#isyeriIdSendika").val(), width: 800, height: 600, label: "İşyeri İşlemleri"});};var showUyeler = function() {	if ($("#isyeriId").val())		AgemWM.window( { id:'csgbIsyeriUyeList', label:'ÇSGB Üye Listesi', icon:'', iconClass:'icon icon-csgb-16-beyaz:icon icon-csgb-16-siyah', url:'genel/csgbIsyeriUye/list.do?listeTurParam=0&isyeriIdParam='+$("#isyeriId").val(), refresh:true, width: 800, height: 600});};var ekle = function() {	AgemWM.submit('isyeriIdSendika', 'genelCsgbIsyeriForm');};	</script>
