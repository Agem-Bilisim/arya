	<script>
var calisanId = ('<bean:write name="p_cleanId"/>' == '1') ? '' : '<bean:write name="p_calisanId"/>';Other.KULLANICI_BILGI_TEK_SATIR = true;$(document).ready(	function () {		Agem.addMaskEvents("calisan.isyeri.sgkKisaNo");		Agem.addElement('input','forward', {type:'hidden', name:'forward', value:'list'}, null, document.forms['genelIsciCalisanForm']);		<logic:notEmpty name='r_message'>		Agem.message('<bean:write name="r_message" filter="false"/>');		</logic:notEmpty>		Agem.byId("kisiId").value = "<bean:write name='p_kisiId'/>";		Agem.byId("calisan.kisiId").value = "<bean:write name='p_kisiId'/>";		Agem.byId("calisan.isci.kisiId").value = "<bean:write name='p_kisiId'/>";		Agem.byId("calisan.isci.kisi.kisiId").value = "<bean:write name='p_kisiId'/>";		if (calisanId == "") {			var f = AgemTable.first();			if (f) {				AgemTable.rowClick(f.id);			}			else {				Agem.ajax("genel/isci/select.do", {json:1,kisiId: <bean:write name='p_kisiId'/>}, function (x) {					var json = x;					AgemWM.addSideInfo(							{								id:"isciInfo",								info: [									{value: json.kisi.ad+" "+json.kisi.soyad, clazz:"pointer", 										onclick: function () {											AgemWM.link('#genelIsciSelect', {url: 'genel/isci/select.do'});										}									},									{label: "T.C.K. No", value: json.kisi.tckNo, clazz: "not-bold"}								],								type: 1							}						);					AgemWM.addQueryString(Agem.json2Param( {kisiId: json.kisi.kisiId, kisiIdParam: json.kisi.kisiId}));				});			}		} else {			AgemTable.rowClick(calisanId);		}			});AgemTable.rowClick = function(id) {	if (!id) {		id = AgemTable.SELECTED_ID;		if (id == null) {			Agem.error("Lütfen kayıt seçiniz.");			return;		}	}		Agem.ajax("genel/calisan/select.do", {json:1,calisanId:id}, function (x) {		Agem.setValue(x, "calisan");		Other.writeKullaniciBilgi();		if (x.calisanIsyeriYetki == '0') {			$('#isyeriBtn').hide();			$('#kaydetBtn').hide();			$('#silBtn').hide();		} else {			$('#isyeriBtn').show();			$('#kaydetBtn').show();			$('#silBtn').show();		}		var json = x.isci;		AgemWM.addSideInfo(				{					id:"isciInfo",					info: [						{value: json.kisi.ad+" "+json.kisi.soyad, clazz:"pointer", 							onclick: function () {								AgemWM.link('#genelIsciSelect', {url: 'genel/isci/select.do'});							}						},						{label: "T.C.K. No", value: json.kisi.tckNo, clazz: "not-bold"}					],					type: 1				}			);		AgemWM.addQueryString(Agem.json2Param( {kisiId: json.kisi.kisiId, kisiIdParam: json.kisi.kisiId}));			});};function yeniKayit() {	Agem.setValue(			 {				 calisanId:'',				 "calisan.calisanId":'',				 "calisan.isyeriId":'',				 "calisan.isyeri.isyeriId":'',				 "calisan.isyeri.unvan":'',				 "calisan.isyeri.sgkKisaNo":'',				 "calisan.girisTarihi":'',				 "calisan.cikisTarihi":'',				 "calisan.cikisNedeni":'',				 "calisan.unvan": '',				 "calisan.aidat": '',				 "calisan.sendikaTemsilci": '',				 "calisan.eklemeTarihi": '',				 "calisan.ekleyen": '',				 "calisan.sonGunlemeTarihi": '',				 "calisan.sonGunleyen": '',				 "calisan.calisanIsyeriYetki":"1"			}	);	$('#isyeriBtn').show();	$('#kaydetBtn').show();	$('#silBtn').show();	Agem.byId("calisan.dayanismaAidati").checked = false;	Agem.byId("calisan.ucretsizIzinde").checked = false;		Agem.byId("calisan.kapsamDisi").checked = false;	Agem.byId("calisan.calisanIsyeriYetki").checked = false;	Other.writeKullaniciBilgi();}function kayitSil() {	if (Agem.byId("calisan.calisanIsyeriYetki").value =="1") {		if (!Agem.byId("calisan.calisanId").value) {			Agem.error("Lütfen kayıt seçiniz.");			return;		}		var x = [				'forward=list',				'calisanId='+Agem.byId("calisan.calisanId").value,				'cleanId=1',								'kisiId='+Agem.byId("kisiId").value			];		if (confirm('Kaydı silmek istediğinizden emin misiniz?\r\nBu kaydı sildiğinizde çalışma kaydıyla ilişkili tüm aidat kayıtları da silinecektir!..')) 			Agem.open( 'genel/isciCalisan/delete.do?'+x.join('&'));	}}var setIsyeri = function (d) {	Agem.setValue(		{			 "calisan.isyeriId":d.id,			 "calisan.isyeri.isyeriId":d.id,			 "calisan.isyeri.unvan":d.unvan,			 "calisan.isyeri.sgkKisaNo":d.sgkKisaNo		}	);};var isyerineGit = function() {	if (Agem.byId("calisan.isyeriId").value && Agem.byId("calisan.calisanIsyeriYetki").value=="1")		AgemWM.window( {link: "genel/isyeri/select.do?isyeriId="+Agem.byId("calisan.isyeriId").value, tab:"#genelIsyeriSelect", width: 800, height: 600, label: "İşyeri Bilgileri", refresh:true});		};	</script>
