	<script>
$(document).ready( function () {	<agem:access URL="genel/aidatSatirTutarFark/edit">	if ($('.exportlinks').length > 0) {		$('<a/>', {     			id: 'kaydet-link',     			href: "javascript:guncelle()",     			title: 'Kayıtları İşyeri Listesine Göre Güncelle'     		}).html("<span title='Kayıtları İşyeri Listesine Göre Güncelle' class='export-icon save-off' onmouseover=\"Other.changeClass(this, 'save-off', 'save-on')\" onmouseout=\"Other.changeClass(this, 'save-on', 'save-off')\"></span>").prependTo('.exportlinks');				$('.save-off').parent().attr("href", "javascript:guncelle()");	}	</agem:access>});var p_aidatId = <bean:write name="p_aidatIdParam"/>;AgemTable.rowDblClick = function(id) {	if (id) {		var d= AgemTable.get(id);		if (d.kisiId) {			AgemWM.window(					{						label: 'İşçi İşlemleri', 						link:"genel/isci/select.do?kisiId=" + d.kisiId + "&kisiIdParam=" +d.kisiId,						width: 800,						height: 600									});		}	}};function guncelle() {	if ($("#liste tr.empty").length > 0) {		alert("Listede işlenecek kayıt yok...");		return;	}	<agem:access URL="genel/aidatSatirTutarFark/edit">	if (!confirm('Kayıtları işyeri listesine göre güncellemek istediğinizden emin misiniz ? (Varolan bilgiler silinecektir)')) {		return;	}	if (p_aidatId) {	   Agem.perde(true, null, 'Lütfen bekleyin...');		$.ajax({"url":"genel/aidatSatirTutarFark/edit.do",			data:{ aidatId: p_aidatId,json:1 },			async : false,			timeout: 30000, 			dataType:"json", 			success:function(data){				if (data['@message']){					Agem.message(data['@message'],'x');				}				if (!data['@hata']) {					Other.displayTagYenile();				}			},			error:function(xhr,err,e){ alert( "Hata: " + err );}});		Agem.perde(false);	}</agem:access><agem:access not="true" URL="genel/aidatSatirTutarFark/edit">	Agem.error("Yetkiniz yok.");</agem:access>};	</script>
