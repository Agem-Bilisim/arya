<arya xmlns="aryaportal.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="aryaportal.org arya.xsd">
	<script>
$(document).ready(function() {	if ($('.exportlinks').length > 0 && '<bean:write name="p_turParam"/>' == 'CSGB_ISYERI') {		<agem:access URL="genel/yetkiAnalizi/edit">			$('<a/>', {     				id: 'kaydet-link',     				href: "javascript:duzenle()",     				title: 'Listelenen Kayıtları Düzenle'     			}).html("<span title='Listelenen Kayıtları Düzenle' class='export-icon save-off' onmouseover=\"Other.changeClass(this, 'save-off', 'save-on')\" onmouseout=\"Other.changeClass(this, 'save-on', 'save-off')\"></span>").prependTo('.exportlinks');					$('.save-off').parent().attr("href", "javascript:duzenle()");		</agem:access>	}	if ($('.exportlinks').length > 0 && '<bean:write name="p_turParam"/>' == 'CSGB_FAZLA_UYE') {				<agem:access  URL="genel/isyeriCsgbAnaliz/edit">					$('<a/>', {     				id: 'kaydet-link',     				href: "javascript:csgbFarkDuzelt()",     				title: 'Farklılıkları Düzelt'     			}).html("<span title='Farklılıkları Düzelt' class='export-icon save-off' onmouseover=\"Other.changeClass(this, 'save-off', 'save-on')\" onmouseout=\"Other.changeClass(this, 'save-on', 'save-off')\"></span>").prependTo('.exportlinks');					$('.save-off').parent().attr("href", "javascript:csgbFarkDuzelt()");		</agem:access>	}	if ($('.exportlinks').length > 0 && '<bean:write name="p_turParam"/>' == 'SENDIKA_FAZLA_UYE') {				<agem:access  URL="genel/isyeriCsgbAnaliz/edit">					$('<a/>', {     				id: 'kaydet-link',     				href: "javascript:sendikaFarkDuzelt()",     				title: 'Üyelikleri Düşür'     			}).html("<span title='Üyelikleri Düşür' class='export-icon save-off' onmouseover=\"Other.changeClass(this, 'save-off', 'save-on')\" onmouseout=\"Other.changeClass(this, 'save-on', 'save-off')\"></span>").prependTo('.exportlinks');					$('.save-off').parent().attr("href", "javascript:sendikaFarkDuzelt()");		</agem:access>	}});	function duzenle() {	if ($("#liste tr.empty").length > 0) {		alert("Listede işlenecek kayıt yok...");		return;	}		<agem:access URL="genel/yetkiAnalizi/edit">		if (!confirm('Şu an listelenen kayıtlar arasındaki tutarsızlıklar düzenlenecektir. Devam edilsin mi ?')) {			return;		}		Agem.perde(true, null, 'Lütfen bekleyin...');		Agem.open('genel/yetkiAnalizi/edit.do?'+				 'forward=list&'+				 'turParam=<bean:write name="p_turParam"/>&' + 				 'isyeriIdParam=<bean:write name="p_isyeriIdParam"/>&' + 				 'reqIsyeriIdParam=<bean:write name="p_reqIsyeriIdParam"/>&' + 				 'subeParam=<bean:write name="p_subeParam"/>&' + 				 'tarihParam=<bean:write name="p_tarihParam"/>');	</agem:access>	<agem:access not="true" URL="genel/yetkiAnalizi/edit">		Agem.error("Yetkiniz yok.");	</agem:access>	}AgemTable.rowDblClick = function(id) {	var d = AgemTable.get(id);	if (d.kisiId)		AgemWM.window( {link: "genel/isci/select.do?kisiId="+d.kisiId, width: 800, height: 600, label: "İşçi İşlemleri"});}var csgbFarkDuzelt = function() {		if ($("#liste tr.empty").length > 0) {		alert("Listede işlenecek kayıt yok...");		return;	}		var currentDate = new Date();	var day = currentDate.getDate();	var month = currentDate.getMonth() + 1;	var year = currentDate.getFullYear();			var today = day + "/" + month + "/" + year;				<agem:access  URL="genel/yetkiAnalizi/edit">		if (!confirm('Şu an listelenen kayıtlar arasındaki tutarsızlıklar düzenlenecektir. Devam edilsin mi ?')) {			return;		}				Agem.open('genel/yetkiAnalizi/edit.do?forward=list&listeTurParam=4&tarihParam='+today+			'&turParam=<bean:write name="p_turParam"/>&'+			'isyeriIdParam=<bean:write name="p_isyeriIdParam"/>');					</agem:access>	};var sendikaFarkDuzelt = function() {		if ($("#liste tr.empty").length > 0) {		alert("Listede işlenecek kayıt yok...");		return;	}		var currentDate = new Date();	var day = currentDate.getDate();	var month = currentDate.getMonth() + 1;	var year = currentDate.getFullYear();			var today = day + "/" + month + "/" + year;				<agem:access  URL="genel/yetkiAnalizi/edit">		if (!confirm('Kayıtlar düşürülecektir. Devam etmek istiyor musunuz?')) {			return;		}				Agem.open('genel/yetkiAnalizi/edit.do?forward=list&listeTurParam=3&tarihParam='+today+			'&turParam=<bean:write name="p_turParam"/>&'+			'isyeriIdParam=<bean:write name="p_isyeriIdParam"/>');					</agem:access>	};	</script>
</arya>
