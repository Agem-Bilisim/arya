<arya xmlns="aryaportal.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="aryaportal.org arya.xsd http://www.zkoss.org/2005/zul/zul.xsd">
	<menubar id="menubar">
		<menu label="İşçi Bilgileri">
			<menupopup>
				<menuitem label="Arama"
					onClick="post('genel.isci.search', 'ALL',{'json': '1'}, 'İşçi Arama')" />
				<menuitem label="Liste"
					onClick="post('genel.isci.list', 'ALL',{'json': '1', pageSize: '1000'}, 'İşçi Listesi')" />
				<menuitem label="Yeni Kayıt"
					onClick="post('genel.isci.emptyIsci', 'VIEW_ONLY',{'json': '1'}, 'İşçi Bilgileri')" />
			</menupopup>
		</menu>
		<menu label="İşyeri Bilgileri">
			<menupopup>
				<menuitem label="Arama"
					onClick="post('genel.isyeri.searchIsyeri', 'VIEW_ONLY',{'json': '1'}, 'İşyeri Arama')" />
				<menuitem label="Liste"
					onClick="post('genel.isyeri.list', 'ALL',{'json': '1'}, 'İşyeri Listesi')" />
				<menuitem label="Yeni Kayıt"
					onClick="post('genel.isyeri.emptyIsyeri', 'VIEW_ONLY',{'json': '1'}, 'İşyeri Bilgileri')" />
			</menupopup>
		</menu>
		<menu label="Üyelik İşlemleri">
			<menupopup>
				<menuitem label="Liste"
					onClick="post('genel.uye.list', 'ALL',{'json': '1'}, 'Üye Listesi')" />
				<menuitem label="Yeni Kayıt"
					onClick="post('genel.uye.emptyUye', 'VIEW_ONLY',{'json': '1'}, 'Üye Bilgileri')" />
			</menupopup>
		</menu>
		<menu label="Yetki Başvuruları">
		</menu>
		<menu label="Toplu İş Sözleşmeleri">
		</menu>
		<menu label="Aidat İşlemleri">
		</menu>
		<menu label="İşveren Sendikaları">
		</menu>
		<menu label="Rakip Sendikalar">
		</menu>
		<menuitem label="Çıkış"
					onClick="post('logout', 'LOGOUT', {'json': '1'})" />
	</menubar>
</arya>